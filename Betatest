local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SITX HUB Beta 1.0", "DarkTheme") -- ‡∏ä‡∏∑‡πà‡∏≠‡∏´‡∏±‡∏ß‡∏Ç‡πâ‡∏≠‡∏´‡∏•‡∏±‡∏Å

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö‡∏´‡∏•‡∏±‡∏Å
local MainTab = Window:NewTab("üè† ‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏Å")
local MainSection = MainTab:NewSection("üîß ‡∏™‡πà‡∏ß‡∏ô‡∏´‡∏•‡∏±‡∏Å")

-- ‡πÄ‡∏û‡∏¥‡πà‡∏° Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏ö‡∏ö Cast ‡πÉ‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏Å
local castToggle
MainSection:NewToggle("üé£ Cast", "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Ç‡∏≠‡∏á‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡πà‡∏ô Cast", function(state)
    if state then
        castToggle = game:GetService("RunService").Stepped:Connect(function()
            -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏≠‡∏∏‡∏õ‡∏Å‡∏£‡∏ì‡πå‡∏õ‡∏£‡∏∞‡πÄ‡∏†‡∏ó Rod ‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
            local rod = nil
            for _, item in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if item:IsA("Tool") and item.Name:match("Rod") then
                    rod = item
                    break
                end
            end

            -- ‡∏ñ‡πâ‡∏≤‡∏û‡∏ö Rod ‡πÉ‡∏´‡πâ‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡∏¢‡∏¥‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á cast
            if rod then
                local args = {
                    [1] = 99.59999999999987, -- ‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡πÉ‡∏ô‡∏Å‡∏≤‡∏£ cast
                    [2] = 1  -- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Ç‡∏≠‡∏á cast
                }
                rod.events.cast:FireServer(unpack(args))
                print("Cast Enabled using: " .. rod.Name)
            else
                print("No rod found!")
            end
        end)
        print("Cast Enabled")
    else
        if castToggle then castToggle:Disconnect() end
        print("Cast Disabled")
    end
end)

-- ‡πÄ‡∏û‡∏¥‡πà‡∏° Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏ö‡∏ö Reel ‡πÉ‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏Å
local reelToggle
MainSection:NewToggle("üé£ Reel", "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Ç‡∏≠‡∏á‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡πà‡∏ô Reel", function(state)
    if state then
        reelToggle = game:GetService("RunService").Stepped:Connect(function()
            local args = {
                [1] = 100,
                [2] = false
            }
            game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("reelfinished"):FireServer(unpack(args))
        end)
        print("Reel Enabled")
    else
        if reelToggle then reelToggle:Disconnect() end
        print("Reel Disabled")
    end
end)

-- ‡πÄ‡∏û‡∏¥‡πà‡∏° Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏ö‡∏ö AutoShake
local autoShakeToggle
local autoShakeDelay = 1  -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡∏Ç‡∏≠‡∏á AutoShake (‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ)
local VirtualInputManager = game:GetService("VirtualInputManager")

MainSection:NewToggle("üîÑ AutoShake", "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Ç‡∏≠‡∏á‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡πà‡∏ô AutoShake", function(state)
    if state then
        autoShakeToggle = game:GetService("RunService").Heartbeat:Connect(function()
            wait(autoShakeDelay)  -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏ï‡∏≤‡∏°‡∏ó‡∏µ‡πà‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤

            local PlayerGUI = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
            local shakeUI = PlayerGUI:FindFirstChild("shakeui")

            if shakeUI and shakeUI.Enabled then
                local safezone = shakeUI:FindFirstChild("safezone")

                if safezone then
                    local button = safezone:FindFirstChild("button")
                    if button and button:IsA("ImageButton") and button.Visible then
                        local pos = button.AbsolutePosition
                        local size = button.AbsoluteSize
                        local centerPos = Vector2.new(pos.X + size.X / 2, pos.Y + size.Y / 2)

                        -- ‡∏à‡∏≥‡∏•‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ó‡∏µ‡πà‡∏õ‡∏∏‡πà‡∏°
                        VirtualInputManager:SendMouseButtonEvent(centerPos.X, centerPos.Y, 0, true, game:GetService("Players").LocalPlayer, 0)
                        VirtualInputManager:SendMouseButtonEvent(centerPos.X, centerPos.Y, 0, false, game:GetService("Players").LocalPlayer, 0)
                    end
                end
            end
        end)
        print("AutoShake Enabled")
    else
        if autoShakeToggle then autoShakeToggle:Disconnect() end
        print("AutoShake Disabled")
    end
end)


-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö "üó∫Ô∏è Teleports"
local TeleportsTab = Window:NewTab("üó∫Ô∏è Teleports")
local TeleportsSection = TeleportsTab:NewSection("üìç Teleport Locations")

-- ‡∏ï‡∏≤‡∏£‡∏≤‡∏á‡πÄ‡∏Å‡πá‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
local teleportLocations = {
    ["Moosewood"] = Vector3.new(400, 135, 250),
    ["Mushgrove Swamp"] = Vector3.new(2420, 135, -750),
    ["Roslit Bay"] = Vector3.new(-1600, 130, 500),
    ["Snowcap Island"] = Vector3.new(2625, 135, 2370),
    ["Statue of Sovereignty"] = Vector3.new(35, 135, -1010),
    ["Sunstone Island"] = Vector3.new(-870, 135, -1100),
    ["Terrapin Island"] = Vector3.new(-95, 130, 1875),
    ["Harvesters Spike"] = Vector3.new(-1260, 135, 1550),
    ["The Arch"] = Vector3.new(1100, 130, -1250),
    ["Birch Cay"] = Vector3.new(1650, 130, -2350),
    ["Haddock Rock"] = Vector3.new(-500, 125, -505),
    ["Earmark Island"] = Vector3.new(1200, 130, 530),
    ["Vertigo"] = Vector3.new(0, 0, 0),
    ["Desolate Deep"] = Vector3.new(-800, 130, -3100),
    ["Forsaken Shore"] = Vector3.new(-2750, 130, 1450),
    ["Archeological Site"] = Vector3.new(4050, 130, 50),
    ["Ancient Isle"] = Vector3.new(6000, 200, 300)
}

local selectedLocation = "Moosewood"

TeleportsSection:NewDropdown("üåê Choose Location", "Select the location to teleport", {"Moosewood", "Mushgrove Swamp", "Roslit Bay", "Snowcap Island", "Statue of Sovereignty", "Sunstone Island", "Terrapin Island", "Harvesters Spike", "The Arch", "Birch Cay", "Haddock Rock", "Earmark Island", "Vertigo", "Desolate Deep", "Forsaken Shore", "Archeological Site", "Ancient Isle"}, function(selected)
    selectedLocation = selected
end)

TeleportsSection:NewButton("üöÄ Teleport", "Teleport to selected location", function()
    local player = game.Players.LocalPlayer
    local location = teleportLocations[selectedLocation]
    if location then
        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(location)
        else
            print("Player or HumanoidRootPart not found")
        end
    else
        print("Invalid location selected")
    end
end)

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö "üõ†Ô∏è Misc (‡∏≠‡∏∑‡πà‡∏ô‡πÜ)"
local MiscTab = Window:NewTab("üõ†Ô∏è Misc (‡∏≠‡∏∑‡πà‡∏ô‡πÜ)")
local MiscSection = MiscTab:NewSection("‚öôÔ∏è Miscellaneous Options")

-- ‡πÄ‡∏û‡∏¥‡πà‡∏° Toggle ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Air Jump ‡πÅ‡∏ö‡∏ö‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î‡πÉ‡∏ô‡∏™‡πà‡∏ß‡∏ô "Misc"
local airJumpToggle
MiscSection:NewToggle("üîÑ Toggle Infinite Air Jump", "Enable/Disable unlimited jumping in air", function(state)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")

    if state then
        airJumpToggle = game:GetService("UserInputService").JumpRequest:Connect(function()
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end)
        print("Infinite Air Jump Enabled")
    else
        if airJumpToggle then airJumpToggle:Disconnect() end
        print("Infinite Air Jump Disabled")
    end
end)

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏±‡∏ß‡∏Ç‡πâ‡∏≠ "üåä Oxygen" ‡πÉ‡∏ô‡πÅ‡∏ó‡πá‡∏ö Misc
local AntiDrown
MiscSection:NewToggle("üåä Infinity Oxygen", "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Infinity Oxygen", function(state)
    AntiDrown = state
    local LocalPlayer = game:GetService("Players").LocalPlayer
    local LocalCharacter = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

    if AntiDrown then
        if LocalCharacter:FindFirstChild("client"):WaitForChild("oxygen") and LocalCharacter.client.oxygen.Enabled == true then
            LocalCharacter.client.oxygen.Enabled = false
        end
        LocalPlayer.CharacterAdded:Connect(function(char)
            if char:FindFirstChild("client"):WaitForChild("oxygen") and AntiDrown then
                char.client.oxygen.Enabled = false
            end
        end)
    else
        if LocalCharacter:FindFirstChild("client"):WaitForChild("oxygen") then
            LocalCharacter.client.oxygen.Enabled = true
        end
    end
end)

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏±‡∏ß‡∏Ç‡πâ‡∏≠ "üèÉ‚Äç‚ôÇÔ∏è Speed" ‡πÉ‡∏ô‡πÅ‡∏ó‡πá‡∏ö Misc
local normalSpeed = 16
local selectedSpeed = 50 -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å, ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏ó‡∏µ‡πà 50

local function setPlayerSpeed(speed)
    local player = game:GetService("Players").LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")

    if humanoid then
        humanoid.WalkSpeed = speed
    end
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Slider ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
MiscSection:NewSlider("üèÉ‚Äç‚ôÇÔ∏è Select Speed", "Adjust the player's walk speed", 16, 100, function(value)
    selectedSpeed = value
    print("Selected speed:", selectedSpeed)
end)

-- ‡πÄ‡∏û‡∏¥‡πà‡∏° Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
MiscSection:NewToggle("üèÉ‚Äç‚ôÇÔ∏è Toggle Speed", "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å", function(state)
    if state then
        setPlayerSpeed(selectedSpeed)
        print("Speed set to:", selectedSpeed)
    else
        setPlayerSpeed(normalSpeed)
        print("Speed set to:", normalSpeed)
    end
end)

-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÉ‡∏´‡πâ‡∏Å‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
setPlayerSpeed(normalSpeed)
print("Initial speed set to:", normalSpeed)

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö "üì∫ Youtube"
local YoutubeTab = Window:NewTab("üì∫ Youtube")
local YoutubeSection = YoutubeTab:NewSection("Youtube ‡∏ä‡πà‡∏≠‡∏á SitXx")
YoutubeSection:NewButton("üëÄ Visit Youtube Channel", "‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ä‡πà‡∏≠‡∏á Youtube ‡∏Ç‡∏≠‡∏á SitXx", function()
    setclipboard("https://www.youtube.com/@SitXx") -- ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏•‡∏¥‡∏á‡∏Ñ‡πå‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏Ñ‡∏•‡∏¥‡∏õ‡∏ö‡∏≠‡∏£‡πå‡∏î
    print("‡∏•‡∏¥‡∏á‡∏Ñ‡πå Youtube ‡∏ñ‡∏π‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏Ñ‡∏•‡∏¥‡∏õ‡∏ö‡∏≠‡∏£‡πå‡∏î‡πÅ‡∏•‡πâ‡∏ß")
end)
